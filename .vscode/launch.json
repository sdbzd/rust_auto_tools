    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "lldb-mi",
            "request": "launch",
            "name": "Launch Program",
            "target": "./bin/executable",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug",
            "program": "${workspaceFolder}/target/debug/daemon.exe",
            "args": ["--bin","dat_json_parse"],
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "rust: cargo build j2s"

        }
    ]
}